<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cristiancastillo.feria_friki">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Exito" />
        <activity android:name=".ExitoSocio" />
        <activity android:name=".HazteSocio" />
        <activity android:name=".HazteSocio2" />
        <activity android:name=".IniciarSesion" />
        <activity android:name=".RegistroSocio" />
        <activity android:name=".RegistroSocio2" />

        <provider
            android:name="com.squareup.picasso.PicassoProvider"
            android:authorities="${applicationId}.com.squareup.picasso"
            android:exported="false" />

        <activity
            android:name=".PerfilUsuario"
            android:label="@string/title_activity_perfil_usuario" />
        <activity android:name=".Credencial" />
        <activity android:name=".Eventos"></activity>
    </application>

</manifest>